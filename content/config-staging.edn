{:site-title           "Practicalli"
 :author               "Practicalli"
 :description          "Discovering the fun in Functional Programming with Clojure"
 :site-url             "http://practical.li/"
 :post-root            "posts"
 :page-root            "pages"
 :post-root-uri        "posts"
 :page-root-uri        "pages"
 :tag-root-uri         "tags"
 :author-root-uri      "authors"
 :public-dest          "public"
 :blog-prefix          "/blog-staging"
 :rss-name             "feed.xml"
 :rss-filters          ["cryogen"]
 :recent-posts         9
 :post-date-format     "yyyy-MM-dd"
 :archive-group-format "yyyy MMMM"
 :sass-src             []
 :sass-path            "sass"
 :compass-path         "compass"
 :theme                "practicalli"
 :resources            ["images" ".nojekyll" "README.org"]
 :keep-files           [".git"]
 :disqus?              true
 :disqus-shortname     "practicalli-blog"
 :ignored-files        [#"\.#.*" #".*\.swp$"]
 :previews?            true
 :posts-per-page       5
 :blocks-per-preview   3
 :clean-urls           :trailing-slash
 :collapse-subdirs?    false
 :hide-future-posts?   false
 :klipse               {:settings {:selector         ".klipse-clj"
                                   :selector-reagent ".klipse-reagent"}}
 :debug?               false}
